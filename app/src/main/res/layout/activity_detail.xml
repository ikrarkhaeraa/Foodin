            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/line_brunch"
            android:layout_width="150dp"
            android:layout_height="8dp"
            android:background="@drawable/rectangle"
            app:layout_constraintBottom_toBottomOf="@+id/bg_blue_detail"
            app:layout_constraintEnd_toEndOf="@+id/bg_blue_detail"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="@+id/bg_blue_detail"
            app:layout_constraintTop_toBottomOf="@+id/brunch"
            app:layout_constraintVertical_bias="0.027"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/food_brunch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/rambla"
            android:text="@string/food_brunch"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/bg_blue_detail"
            app:layout_constraintHorizontal_bias="0.037"
            app:layout_constraintStart_toStartOf="@+id/bg_blue_detail"
            app:layout_constraintTop_toBottomOf="@+id/line_brunch"
            app:layout_constraintVertical_bias="0.025"
            tools:ignore="MissingConstraints" />

        <TextView
